local a=game:GetService("Players")local b=a.LocalPlayer;local c=b:FindFirstChild("OtherData")local d=500;if c then local e=0;for f,g in ipairs(c:GetDescendants())do if string.find(g.Name,"^Slot(%d+)$")then e=e+1 end end;if e>=d then warn("Maximum slot limit reached.")return end;local h=_G.Amount or 0;h=math.floor(h)h=math.max(0,h)local i=math.min(h,d-e)if i<=0 then warn("No slots to add.")return end;local j=0;for f,g in ipairs(c:GetDescendants())do local k=tonumber(string.match(g.Name,"^Slot(%d+)$"))if k then j=math.max(j,k)end end;for l=1,i do local m=j+l;task.spawn(function()workspace:WaitForChild("UpdateSlotOrderRE"):FireServer(1,m.."\0",0,"")end)end end
